<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>

  <appSettings file="private.config" >
    <add key="BPFeedUrl" value="http://lda.data.parliament.uk/briefingpapers.json"/>

    <add key="Twitter.AccountName" value="(insert account name here)" />
    <add key="Twitter.ConsumerKey" value="(insert consumer key here)" />
    <add key="Twitter.ConsumerSecret" value="(insert consumer secret here)" />
    <add key="Twitter.AccessToken" value="(insert access token here)" />
    <add key="Twitter.AccessTokenSecret" value="(insert access token secret here)" />
  </appSettings>

</configuration>